{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/igclib/c_ext/c_dev",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "dev",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/igclib/bin/igclib",
            "args": ["race", "--task", "pwca_task.json"],
            "console": "integratedTerminal"
        },
        {
            "name": "race",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/igclib/bin/igclib",
            "args": ["race", "--task", "igclib/tests/test_data/tasks/task.xctsk", "--flights", "igclib/tests/test_data/one_track", "--output", "saved_races/small_race.json"],
            "console": "integratedTerminal"
        },
        {
            "name": "json race",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/igclib/bin/igclib",
            "args": ["race", "--task", "igclib/tests/test_data/tasks/pwca_task.json"],
            "console": "integratedTerminal"
        },
        {
            "type": "python",
            "request": "launch",
            "name": "current",
            "program": "${file}"
          }
    ]
}