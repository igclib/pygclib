{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/igclib/c_ext/c_dev",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "dev",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/igclib/main.py",
            "args": ["--race", "test_data/regression.pkl", "--pilot", "0035"],
            "console": "integratedTerminal"
        },
        {
            "name": "export",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/igclib/bin/igclib",
            "args": ["--mode", "race", "--task", "test_data/tasks/task.xctsk", "--flights", "test_data/one_track", "--output", "/dev/null"],
            "console": "integratedTerminal"
        },
        {
            "type": "python",
            "request": "launch",
            "name": "current",
            "program": "${file}"
          }
    ]
}